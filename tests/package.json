{
  "name": "pawfinds-selenium-tests",
  "version": "1.0.0",
  "description": "Automated Selenium tests for PawFinds Pet Adoption System",
  "main": "index.js",
  "scripts": {
    "test": "mocha tests/**/*.test.js --timeout 30000 --exit",
    "test:report": "mocha tests/**/*.test.js --timeout 30000 --reporter mochawesome --reporter-options reportDir=reports,reportFilename=test-report --exit"
  },
  "keywords": [
    "selenium",
    "testing",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@wdio/selenium-standalone-service": "^8.14.0",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "selenium-webdriver": "^4.15.0"
  }
}
